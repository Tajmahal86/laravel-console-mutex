{
    "name": "illuminated/console-mutex",
    "description": "Prevents overlapping for Laravel console commands.",
    "keywords": ["laravel", "console", "command", "mutex", "overlapping", "locker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/console": "^5.2",
        "arvenil/ninja-mutex": "^0.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "orchestra/testbench": "^3.3",
        "mockery/mockery": "^0.9.5"
    },
    "suggest" : {
        "predis/predis": "If you want to use `redis` strategy."
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/fixture/app"
        ]
    }
}
